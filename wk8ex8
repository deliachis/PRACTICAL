from math import sqrt
def f(x) :
    return g(x) + sqrt(h(x))
def g(x) :
    return 4 * h(x)
def h(x) :
    return x * x + k(x) - 1
def k(x) :
    return 2 * (x + 1)

print('a. answer is: ',f(2))
print('b. answer is: ',g(h(2)))
print('c. answer is: ',k(g(2)+h(2)))
print('d. answer is: ',f(0)+f(1)+f(2))
print('e. answer is: ',f(-1)+g(-1)+h(-1)+k(-1))
